{
    "name": "garmin-api-handler",
    "version": "0.2.0",
    "description": "Unofficial handler for Garmin connect API",
    "main": "dist/index.js",
    "jsnext:main": "dist/index.esm.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "lint": "eslint --ext .ts ./src ./tests",
        "lint:export": "npm run lint -- -o ./tests_results/checkstyle/js-checkstyle.xml -f checkstyle",
        "tsc": "tsc",
        "develop": "tsc --emitDeclarationOnly false --watch",
        "build": "rollup --config ./rollup.config.js",
        "test": "jest",
        "test:build": "npm run test -- --config jest.config.build.js",
        "test:all": "npm run lint && npm run tsc && npm run test",
        "prepublishOnly": "npm run build && npm run tsc && npm run test:build",
        "release": "semantic-release"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/fabulator/garmin-api-handler.git"
    },
    "author": "Michal Ozog√°n <michal@ozogan.eu>",
    "license": "Apache-2.0",
    "homepage": "https://github.com/fabulator/garmin-api-handler#readme",
    "keywords": [
        "garmin",
        "garmin-api"
    ],
    "devDependencies": {
        "@socifi/babel-config": "^0.6.1",
        "@socifi/browserslist-config": "^0.1.0",
        "@socifi/commitlint-config": "^1.0.0",
        "@socifi/eslint-config": "^2.7.1",
        "@socifi/jest-config": "^2.3.1",
        "@socifi/rollup-config": "^2.1.0",
        "husky": "^1.3.1",
        "semantic-release-config-fabulator": "^1.0.3"
    },
    "dependencies": {
        "@types/form-data": "^2.2.1",
        "@types/luxon": "^1.12.0",
        "@types/mathjs": "^5.0.1",
        "cookie-api-handler": "^2.1.0",
        "fitness-models": "^0.4.0",
        "form-data": "^2.3.3",
        "luxon": "^1.12.1",
        "mathjs": "^5.8.0",
        "rest-api-handler": "^2.1.0"
    }
}
